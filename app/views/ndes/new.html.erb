<h1> Create a new Near Death Experience </h1>
<%= simple_form_for(@nde) do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :city %>
<%= f.input :participant_number %>
<%= f.input :location %>
<%= f.input :drama %>
<%= f.input :price %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.submit "Create" %>
<% end %>
